{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*.{js,css,png,jpg,gif}", "/.well-known/*"]
  },
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "must-revalidate, max-age=15770000"
      }
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "platform": {
    "apiRuntime": "node:20"
  }
}